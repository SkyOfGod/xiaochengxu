.section{ position: absolute; width: 100%; height: 93% ; overflow: hidden;}
.left{ position: absolute; left: 0; top: 0; height: 100%; width: 90px; background: #F6F6F6; text-align: center;font-size:30rpx;}
.left::before{ content: ""; position: absolute; background: #dcdcdc; right: 0; top: 0; height: 100%; }
.left view.active{ background: #FFF; position: relative; z-index: 2;}
/* .left view{ display: block; line-height: 60px;} */

.right{ margin-left: 90px; height: 95%; overflow: hidden; width: auto;background: #fff;}
.right text{ font-size: 15px}
.typeTree{ padding-top: 10px;width: 100%}
.typeTree .item{  text-align: center; display: inline-block; vertical-align: top;}
.typeTree .brand,
.typeTree .category{ flex-wrap: wrap; overflow: hidden;display: inline-block;width: 40%;margin-left: 6%}
.typeTree .title{ padding: 10px 0; display: block; color: #333;}
.typeTree image { width: 100px; margin: 0 auto;}
.typeTree .brand image{ height: 100px;}
.typeTree .category image{ height: 85px;}
.typeTree .category{ padding-bottom: 10px;}
.item text{ padding-bottom: 10px; display: block;}

.index-bd {
  padding: 0 20rpx 40rpx;
}
.kind-list-item-hd {
  padding: 30rpx;
  display: flex;
  align-items: center; 
}

.kind-list-item-bd {
  height: 0;
  overflow: hidden;
}
.kind-list-item-bd-show {
  height: auto;
}
.navigator-box {
  opacity: 0;
  position: relative;
  line-height: 1.2;
  font-size: 30rpx;

  transform: translateY(-50%);
  transition: .3s;
}
.navigator-box-show {
  opacity: 1;
  transform: translateY(0);
}
.navigator-text{
  height: 30px;
}

.sh_slt{
  width: 80px;
  height: 70px;
  margin-right: 8%;
  float: left;
}
.right-list{
  border-color:#998 ;
  border-width: 2px;
  border-bottom-style: solid;
  margin: 1px 0 1px;
  padding: 5px 0 5px;
}
.shop{
  background: #fff;
  padding: 4%;
  margin-bottom: 10px;
}
.df{
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
}
.df_1{
  font-size: 12px;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  -ms-flex: 1;
  flex: 1;
  -webkit-tap-highlight-color: transparent;
}
.scimg{
    width: 100px;
    height: 100px;
    display: inline-block;
    background-size:100px; 
}
.search_no{
    width: 100%; 
    display: block;
    color: #666;
    text-align: center;
    font-size: 14px;
}
.right-button{
  display: flex;
}
.right-button button{
  width: 48%;
  height: 40%;
  font-size: 12px;
  margin: 0 3px 0;
  color: blue;
  background-color: rgb(192, 192, 238); 
}
.productSearch{
  display: flex;
  padding: 5px 5px;
}
.product-input{
  margin-left:86px;
  margin-right: 12px;
  background-color: white;
  width: 50%;
}
.productSearchBtn{
  background-color: rgb(192, 192, 238); 
  border-radius: 5px;
}
.productSearchBtn-text{
  padding: 5px 5px;
}

.hide {
  display: none !important;
}
.show {
  display: block !important;
}
.set-store-price {
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 250;
  background: rgba(33, 33, 33, 0.7);
}
.set-store-price-form {
  width: 662rpx;
  min-height: 466rpx;
  background: #fff;
  border-radius: 10rpx;
  box-shadow: 0 8px 16px 0 rgba(0, 0, 0, 0.4);
  position: fixed;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  z-index: 1000;
}
.set-store-price-form-box {
  padding: 40rpx 60rpx;
}
.set-store-price-tit {
  font-size: 36rpx;
  color: #212121;
  text-align: center;
  font-weight: bold;
}
.set-store-price-store{
  display: flex;
  width: 542rpx;
  margin: 10px 10px;
}
.set-store-price-store-block{
  margin-left: 40px;
  margin-right: 40px;
  border:1px rgb(192, 192, 238) solid;
  border-radius:2px;
  width:40%;
  display:flex;
}
.set-store-price-store-btn{
  padding-left: 12px;
  padding-right: 12px;
}
.set-store-price-store input{
  width: 50px;
  border-left:1px rgb(192, 192, 238) solid;
  border-right: 1px rgb(192, 192, 238) solid;
}
.set-store-price-price-block{
  margin-left: 40px;
  margin-right: 40px;
  border:1px rgb(192, 192, 238) solid;
  border-radius:2px;
  width:40%;
  display:flex;
}
.set-store-price-price-block input{
  width: 100px;
}
.set-store-price-btn{
  display: flex;
}
.set-store-price-btn button{
  width: 50%;
}



